{
  "name": "cluster-api-refactory",
  "version": "1.0.0",
  "description": "",
  "main": "bootstrap.js",
  "type": "module",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node src/node_server/bootstrap.js",
    "dev": "nodemon --experimental-specifier-resolution=node src/node_server/bootstrap.js",
    "cluster": "node --experimental-specifier-resolution=node src/node_server/cluster.js",
    "test": "npx jest --runInBand --forceExit --silent -- cluster.spec",
    "test:load": "npx autocannon -c 500 -d 30 --workers 10 --renderStatusCodes --latency --warmup [ -c 1 -d 2 ] localhost:3000/success",
    "posttest": "npx jest --runInBand --forceExit --silent -- posttest",
    "start:express": "node --experimental-specifier-resolution=node src/express/bootstrap.js",
    "dev:express": "nodemon --experimental-specifier-resolution=node src/express/bootstrap.js",
    "cluster:express": "node --experimental-specifier-resolution=node src/express/cluster.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "autocannon": "^7.10.0",
    "express": "^4.18.2",
    "jest": "^29.4.1",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3"
  }
}
